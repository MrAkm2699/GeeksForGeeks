/******************CODE***********/
int main()
 {
     ios_base::sync_with_stdio(0);
    cin.tie(0);
 	int t;
    cin>>t;
    while(t--){
    	long long int n,k;
    	cin>>n>>k;
    	while(n%10 > 0 && k)
            {
                n ^= n%10;
                k--;
            }
    	cout<<n<<"\n";
    }
	//code
	return 0;
}
